{
  "name": "Jironimo",
  "version": "1.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "/icons/16.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "browser_action": {
    "default_icon": "/icons/128.png",
    "default_popup": "/views/default.html"
  },
  "permissions": [
    "http://*/",
    "tabs",
    "cookies",
    "notifications"
  ]
}
