{
  "name": "Jironimo",
  "version": "1.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "16": "/icons/16.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "browser_action": {
    "default_icon": "/icons/128.png",
    "default_popup": "/app/layouts/popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications"
  ]
}
